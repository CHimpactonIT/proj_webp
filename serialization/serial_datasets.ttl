    dct:language "en" ;
    dct:license <https://creativecommons.org/licenses/by/4.0/> ;
    dct:publisher "Eurostat" ;
    dct:rightsHolder "Eurostat" ;
    dct:title "Establishments, bedrooms, and bed-places in tourist accommodation (NUTS 2 region)"@en ;
    dcat3:distribution ".csv, .tsv, .xlsx, SDMX (.xml, .json)" ;
